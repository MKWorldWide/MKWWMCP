        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "editorMode": "code",
          "expr": "rate(node_gc_duration_seconds_sum[1m]) / rate(node_gc_duration_seconds_count[1m]) * 1000",
          "hide": false,
          "legendFormat": "GC Duration (ms) - {{instance}}",
          "range": true,
          "refId": "C"
